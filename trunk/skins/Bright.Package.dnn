					<moduleControl>
						<controlKey>DNNRADMENU</controlKey>
						<controlSrc>Admin/Skins/DNNRadMenu/DNNRadMenu.ascx</controlSrc>
						<supportsPartialRendering>True</supportsPartialRendering>
					</moduleControl>
				</component>
				<component type="File">
					<files>
						<basePath>Admin/Skins/DNNRadMenu</basePath>
